{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "npm: build",
      "args": ["-i", "sample-recalls.csv", "-o", "out-recalls.csv", "--debug"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}